# GitHub App Configuration
APP_ID=your_app_id_here
WEBHOOK_SECRET=your_webhook_secret_here
PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----
your_private_key_here
-----END RSA PRIVATE KEY-----"

# Smee Webhook Proxy (for local development)
SMEE_URL=https://smee.io/your_channel_here
WEBHOOK_PROXY_URL=https://smee.io/your_channel_here

# OpenAI API (fallback if Gemini not set)
OPENAI_API_KEY=sk-your_openai_key_here

# Gemini API (preferred - set this to use Gemini instead of OpenAI)
GEMINI_API_KEY=your_gemini_api_key_here

# ChromaDB (optional, defaults to localhost)
CHROMADB_URL=http://localhost:8000
